#!/usr/bin/perl
#

use strict;
use warnings;
use Data::Dumper;

BEGIN {
    $QVD::Config::USE_DB = 0;
    @QVD::Config::FILES = ('/etc/qvd/vma.conf');
}

use QVD::Config;
use QVD::Log;
use QVD::VmaHook::Passthrough qw(save_credentials);

DEBUG "The arguments are:".Dumper(\@ARGV);
my $result = save_credentials(\@ARGV);
my $exit_value = ($result ? 0 : 1);
exit $exit_value;

=head1 qvd_passthrough_hook

=head1 DESCRIPTION

See L<QVD::VmaHook::Passthrough>

=head1 AUTHOR

QVD Team, C<< <qvd-devel at theqvd.com> >>

